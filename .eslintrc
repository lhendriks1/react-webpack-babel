{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "airbnb/hooks", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": 1,
    "import/no-extraneous-dependencies": 1,
    "camelcase": 1,
    "import/first": 1,
    "no-unused-vars": 1,
    "react/prop-types": 1,
    "react/jsx-filename-extension": 0
  }
}
